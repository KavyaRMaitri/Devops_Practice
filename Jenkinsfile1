pipeline {
    agent any
    
    environment {
        // Define environment variables to store secret values
        num1 = credentials('secret1')
        num2 = credentials('secret2')
    }
    
    stages {
        stage('Stage 2') {
            steps {
               withCredentials([string(credentialsId: 'secret1', variable: 'num1'), string(credentialsId: 'secret2', variable: 'num2')]) {
               sh "S1.py ${num1} ${num2}"
}
}
}
}
}
